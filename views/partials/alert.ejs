
<div class="m-2">
    <% if(flashMessage.success){ %>
        <div class="alert alert-success">
            <%= flashMessage.success %>
            <button class="close" data-dismiss ="alert">&times;</button>
        </div>
    <% }else if(flashMessage.fail){ %>
        <div class="alert alert-danger">
            <%= flashMessage.fail %>
            <button class="close" data-dismiss="alert">&times;</button>
        </div>
    <% } %>
</div>